{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "api/test.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/test",
      "destination": "/api/test.py"
    }
  ],
  "crons": [
    {
      "path": "/api/v1/worker/cron/process",
      "schedule": "*/5 * * * *"
    }
  ]
}
